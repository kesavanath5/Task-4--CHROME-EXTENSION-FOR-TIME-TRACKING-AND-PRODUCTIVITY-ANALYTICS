
{
  "manifest_version": 3,
  "name": "TimeTracker Productivity",
  "version": "1.0",
  "description": "Tracks time spent on websites and provides productivity analytics.",
  "permissions": ["tabs", "storage", "activeTab", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
